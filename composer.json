{
    "name": "joesama/pintu",
    "description": "Another utilities package for routing & policy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://github.com/joesama/pintu"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/console": "^6.0",
        "illuminate/support": "^6.0",
        "illuminate/routing": "^6.0",
        "illuminate/filesystem": "^6.0",
        "symfony/finder": "^4.3",
        "nunomaduro/laravel-console-menu": "^2.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.2.3",
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Joesama\\Pintu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joesama\\Pintu\\PintuProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}