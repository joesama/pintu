{
    "name": "joesama/pintu",
    "description": "Another utilities package for routing & policy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://github.com/joesama/pintu"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/support": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/filesystem": "^8.0",
        "symfony/finder": "^6.0",
        "nunomaduro/laravel-console-menu": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3",
        "orchestra/testbench": "^6.0",
        "mockery/mockery": "^1.4.2",
        "orchestra/canvas": "^6.0",
        "phpstan/phpstan": "^0.12.33"
    },
    "autoload": {
        "psr-4": {
            "Joesama\\Pintu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joesama\\Pintu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joesama\\Pintu\\PintuProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
