{
    "name": "joesama/pintu",
    "description": "Another utilities package for routing & policy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Johari Jumali",
            "email": "joharijumali@gmail.com",
            "homepage": "https://github.com/joesama/pintu"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/console": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/filesystem": "^7.0",
        "symfony/finder": ">=4.0",
        "nunomaduro/laravel-console-menu": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "orchestra/testbench": "^5.1",
        "mockery/mockery": "^1.3",
        "orchestra/canvas": "^5.2",
        "phpstan/phpstan": "^0.12.20"
    },
    "autoload": {
        "psr-4": {
            "Joesama\\Pintu\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joesama\\Pintu\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Joesama\\Pintu\\PintuProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
